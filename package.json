{
  "name": "spoons",
  "description": "The unofficial Wetherspoons passport app",
  "version": "0.0.114",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "phone": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch --onlyChanged",
    "test:ci": "jest --runInBand",
    "test:server": "jest --config jest.server.config.ts",
    "ssr": "ng build && npm run serve:ssr",
    "release": "standard-version",
    "analyze": "source-map-explorer dist/spoons/browser/*.js",
    "analyze:webpack": "webpack-bundle-analyzer dist/spoons/browser/stats.json",
    "analyze:both": "npm run analyze && npm run analyze:webpack",
    "build:stats": "ng build --stats-json",
    "bundle-report": "node tools/performance/bundle-analysis.js",
    "dev:metrics": "npm run build && npm run bundle-report",
    "build:firebase": "ng build --configuration production",
    "deploy": "npm run build:firebase && firebase deploy",
    "format": "prettier --write \"src/**/*.{ts,js,html,scss,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,html,scss,css,json}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,js,html,scss,css,json}\" && npm run format:organize-imports",
    "format:organize-imports": "npx organize-imports-cli \"src/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "20.1.2",
    "@angular/common": "^20.1.2",
    "@angular/compiler": "^20.1.2",
    "@angular/core": "^20.1.2",
    "@angular/fire": "20.0.1",
    "@angular/forms": "^20.1.2",
    "@angular/platform-browser": "^20.1.2",
    "@angular/platform-browser-dynamic": "^20.1.2",
    "@angular/platform-server": "^20.1.2",
    "@angular/router": "^20.1.2",
    "@angular/service-worker": "^20.1.2",
    "@angular/ssr": "^20.1.1",
    "@fourfold/angular-foundation": "0.1.10",
    "@google/generative-ai": "0.24.1",
    "@jest/globals": "29.7.0",
    "compression": "1.8.0",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "debug": "4.4.1",
    "dotenv": "16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "7.5.0",
    "fast-xml-parser": "5.2.3",
    "firebase": "11.8.1",
    "lottie-web": "5.13.0",
    "nanoid": "5.1.5",
    "ngx-lottie": "13.0.1",
    "qrcode": "1.5.4",
    "redis": "5.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "undefined": "0.1.0",
    "uuid": "11.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "20.0.0",
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.2",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.8",
    "@types/cors": "2.8.18",
    "@types/debug": "4.1.12",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.14",
    "@types/node": "18.19.104",
    "@types/qrcode": "1.5.5",
    "@types/supertest": "6.0.3",
    "browser-sync": "3.0.4",
    "firebase-admin": "13.4.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-preset-angular": "14.6.0",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.2.0",
    "source-map-explorer": "2.5.3",
    "standard-version": "9.5.0",
    "supertest": "7.1.1",
    "ts-jest": "29.3.4",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "webpack-bundle-analyzer": "4.10.2"
  },
  "engines": {
    "node": "20.x"
  }
}
